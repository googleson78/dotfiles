-- module imports are annoying
:set prompt "> "
-- also annoying in multiline stuff
:set prompt-cont ">|"

-- allow multiline stuff
:set +m

-- fully qualified module names are annoying
:set -dsuppress-module-prefixes

:set -XOverloadedStrings
:set -XTypeApplications

import qualified Data.Text as T
import qualified Data.Text.IO as T

import qualified Data.ByteString as BS
